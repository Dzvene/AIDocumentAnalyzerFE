# API Configuration
REACT_APP_API_URL=http://api.clearcontract.io
REACT_APP_API_TIMEOUT=30000
VITE_API_BASE_URL=http://api.clearcontract.io/api

# WebSocket Configuration (optional)
REACT_APP_WS_URL=ws://api.clearcontract.io

# App Configuration
VITE_APP_NAME=AI Document Analyzer
VITE_APP_VERSION=1.0.0
REACT_APP_VERSION=${npm_package_version}
REACT_APP_ENV=production

# Environment
NODE_ENV=production

# Google OAuth Configuration
REACT_APP_GOOGLE_CLIENT_ID=your-google-client-id-here
VITE_GOOGLE_CLIENT_ID=your-google-client-id-here

# Stripe Configuration
REACT_APP_STRIPE_PUBLISHABLE_KEY=pk_test_your-stripe-key-here
VITE_STRIPE_PUBLISHABLE_KEY=pk_test_your-stripe-key-here

# PayPal Configuration
VITE_PAYPAL_CLIENT_ID=your_paypal_client_id

# Feature Flags
REACT_APP_ENABLE_ANALYTICS=true
REACT_APP_ENABLE_DEBUG=false
VITE_ENABLE_ANALYTICS=true
VITE_ENABLE_PWA=true

# CDN Configuration (optional)
REACT_APP_CDN_URL=https://cdn.example.com

# Sentry Configuration (optional)
REACT_APP_SENTRY_DSN=https://your-sentry-dsn@sentry.io/project-id

# Analytics Configuration (optional)
REACT_APP_GA_TRACKING_ID=UA-XXXXXXXXX-X

# Maximum file upload size (in bytes)
REACT_APP_MAX_FILE_SIZE=10485760

# Supported file types for document upload
REACT_APP_SUPPORTED_FILE_TYPES=.pdf,.doc,.docx,.txt,.rtf

# Localization
REACT_APP_DEFAULT_LANGUAGE=ru
REACT_APP_SUPPORTED_LANGUAGES=ru,en,es,de,fr,it,pl